<%- include('./includes/head.ejs') %>
<link rel="stylesheet" href="<%= cssFile %>">
<script src="<%= scriptFile %>" defer></script>
<title><%= pageTitle %></title>
</head>
<%- include('./includes/body.ejs') %>
      <div class="game-play-hint">
        <p class="hint-text"><%= hintText %></p>
      </div>
    </div>
    <div class="central-layout">
      <div class="game">
        <div class="container">
          <div class="quote-display" id="quoteDisplay"></div>
          <textarea id="quoteInput" class="quote-input" placeholder="Start typing..." autofocus></textarea>
        </div> 
      </div>
    </div>
    <div class="right-layout">
      <div class="computer-response-text"><%= computerResponse %></div>
      <div class="computer-response-image"><img src="<%= svgPath %>" alt=""></div>
    </div>
  </div>
  <div class="cta-btn-wrapper">
    <%- include('./includes/cta-btn') %>
  </div>
</div>
</body>
</html>


