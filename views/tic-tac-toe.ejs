<%- include('./includes/head.ejs') %>
<link rel="stylesheet" href="<%= cssFile %>">
<script src="<%= scriptFile %>" defer></script>
<title><%= pageTitle %></title>
</head>
<%- include('./includes/body.ejs') %>
      <div class="game-play-hint">
        <p class="hint-text"><%= hintText %></p>
      </div>
    </div>
    <div class="central-layout">
      <div class="game">
        <table>
          <tr>
            <td class="cell" id="0"></td>
            <td class="cell" id="1"></td>
            <td class="cell" id="2"></td>
          </tr>
          <tr>
            <td class="cell" id="3"></td>
            <td class="cell" id="4"></td>
            <td class="cell" id="5"></td>
          </tr>
          <tr>
            <td class="cell" id="6"></td>
            <td class="cell" id="7"></td>
            <td class="cell" id="8"></td>
          </tr>
        </table> 
      </div>
    </div>
    <div class="right-layout">
      <div class="computer-response-text"><%= computerResponse %></div>
      <div class="computer-response-image"><img src="<%= svgPath %>" alt=""></div>
    </div>
  </div>
  <div class="cta-btn-wrapper">
    <%- include('./includes/cta-btn') %>
  </div>
</div>
</body>
</html>

